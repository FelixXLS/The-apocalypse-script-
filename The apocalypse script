end)

tab.newButton("Ammo", "spawn", function()

workspace.Items.Ammo.Handle.InteractEvent:FireServer()
end) 

tab.newButton("medkit", "spawn", function()

workspace.Items.Medkit.Handle.InteractEvent:FireServer()
